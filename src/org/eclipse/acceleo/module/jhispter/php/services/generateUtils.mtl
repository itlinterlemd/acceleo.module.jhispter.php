[module generateUtils('http://www.github.io/jhipster/jdl/JDL')]


[query public isClassValid(aJdlEntity : JdlEntity) : Boolean =
	if aJdlEntity.name.contains('Authority') or aJdlEntity.name.contains('User') then
		false
	else
		true
	endif
